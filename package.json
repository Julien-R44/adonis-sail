{
  "name": "adonis-sail",
  "description": "â›µ Generate a ready-to-use docker environment for your Adonis application",
  "version": "1.0.0-beta.2",
  "license": "MIT",
  "type": "module",
  "author": "Julien Ripouteau <julien@ripouteau.com>",
  "files": [
    "index.ts",
    "configure.ts",
    "src",
    "factories",
    "services",
    "providers",
    "commands",
    "build/stubs",
    "build/configure.js",
    "build/configure.d.ts",
    "build/configure.d.ts.map",
    "build/index.js",
    "build/index.d.ts",
    "build/index.d.ts.map",
    "build/src",
    "build/services",
    "build/providers",
    "build/factories",
    "build/commands/"
  ],
  "exports": {
    ".": "./build/index.js",
    "./commands": "./build/commands/main.js",
    "./commands/*": "./build/commands/*.js"
  },
  "main": "build/index.js",
  "scripts": {
    "pretest": "pnpm lint",
    "test": "c8 pnpm quick:test",
    "lint": "eslint .",
    "clean": "del-cli build",
    "compile": "pnpm lint && pnpm clean && tsc && pnpm copy:templates && pnpm index:commands",
    "copy:templates": "copyfiles \"stubs/**/*.stub\" build",
    "build": "pnpm compile",
    "version": "pnpm build",
    "quick:test": "node --loader=ts-node/esm bin/test.ts",
    "release": "bumpp --commit \"chore(release): %s\" --push --tag && pnpm publish",
    "format": "prettier --write .",
    "prepublishOnly": "pnpm build",
    "index:commands": "adonis-kit index build/commands"
  },
  "devDependencies": {
    "@adonisjs/core": "^6.1.5-4",
    "@japa/assert": "^1.4.1",
    "@japa/expect-type": "^1.0.3",
    "@japa/file-system": "^1.0.1",
    "@japa/run-failed-tests": "^1.1.1",
    "@japa/runner": "^2.5.1",
    "@japa/snapshot": "1.0.1-2",
    "@japa/spec-reporter": "^1.3.3",
    "@julr/tooling-configs": "^2.1.0",
    "@swc/core": "^1.3.56",
    "@types/node": "^20.1.0",
    "bumpp": "^9.1.0",
    "c8": "^7.13.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "@adonisjs/core": "^6.1.5-4"
  },
  "dependencies": {
    "@poppinss/utils": "^6.5.0-2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julien-R44/adonis-sail.git"
  },
  "bugs": {
    "url": "https://github.com/Julien-R44/adonis-sail/issues"
  },
  "homepage": "https://github.com/Julien-R44/adonis-sail#readme",
  "keywords": [
    "adonisjs",
    "adonis5",
    "adonis6",
    "docker",
    "adonis-sail",
    "docker-compose"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "eslintIgnore": [
    "build"
  ],
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "prettier": "@julr/tooling-configs/prettier"
}
